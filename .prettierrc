{
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "semi": true,
  "useTabs": false,
  "tabWidth": 4,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "jsxBracketSameLine": true,
  "htmlWhitespaceSensitivity": "strict",
  "plugins": ["prettier-plugin-organize-imports"],
  "printWidth": 70,
  "endOfLine": "lf"
}
